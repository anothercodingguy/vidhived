# Example .env file for Vidhived.ai
# Do NOT commit this file to version control.

# Your Google Cloud Project ID
GCP_PROJECT_ID="your-gcp-project-id"

# The region for your GCP services (e.g., us-central1)
GCP_REGION="us-central1"

# The name of the Google Cloud Storage bucket to store uploads and results
GCS_BUCKET_NAME="your-unique-bucket-name-for-vidhived"

# The path to your GCP service account key JSON file.
# Ensure this file is kept secure.
GOOGLE_APPLICATION_CREDENTIALS="./your-gcp-service-account-key.json"
